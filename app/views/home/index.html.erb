<section id="warehouses">
<h2>Galpões</h2>
<div>
  <%# <%= flash[:notice] %> 
  <%# <%= notice %>
</div>
<div>
<%= link_to('Cadastrar Galpão', new_warehouse_path) %>
</div>
<% @warehouses.each do |w| %>
 
<dl>
  <dt><%= link_to(w.name, warehouse_path(w.id))%></dt>
    <%# <%= link_to(w.name, warehouse_url(w.id))%> 
  <dt>Código:</dt>
  <dd><%= w.code %></dd>

  <dt>Cidade: </dt>
  <dd><%= w.city %></dd>

  <dt>Área: </dt>
  <dd><%= w.area %> m2</dd>
</dl>

<% end %>
</section>

<% if @warehouses.empty? %>
  <p>Não existe galpões cadastrados</p>
<% end %>